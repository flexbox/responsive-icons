    i class="bq-pointer-left"         entity-name="lsaquo;" entity-hex="\2039"
    i class="bq-pointer-right"        entity-name="rsaquo;" entity-hex="\203A"
    i class="bq-pointer-double-left"  entity-name="laquo;"  entity-hex="\00AB"
